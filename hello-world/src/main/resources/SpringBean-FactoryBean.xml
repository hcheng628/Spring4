<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="carFactoryBean" class="us.supercheng.spring4.factorybean.entity.CarFactoryBeanImpl" scope="prototype">
		<constructor-arg index="0" value="BMW"></constructor-arg>
		<constructor-arg index="1" value="Z4"></constructor-arg>
		<constructor-arg index="2" value="70000"></constructor-arg>
		<constructor-arg index="3" value="300"></constructor-arg>
	</bean>
</beans>